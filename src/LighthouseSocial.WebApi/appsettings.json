{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Minio": {
    "Endpoint": "localhost:9008",
    "BucketName": "lighthouse-bucket",
    "UseSSL": false
  },
  "VaultSettings": {
    "Address": "http://localhost:8200",
    "Token": "root",
    "MountPoint": "secret"
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "Caching": {
    "UseRedis": true,
    "InstanceName": "LighthouseSocial:"
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200",
    "IndexFormat": "lighthouse-social-logs-{0:yyyy.MM.dd}",
    "Username": "",
    "Password": ""
  },
  "GraylogSettings": {
    "Host": "localhost",
    "Port": 12201,
    "Facility": "lighthouse-social",
    "Environment": "Development",
    "UseSecureConnection": false
  }
}
